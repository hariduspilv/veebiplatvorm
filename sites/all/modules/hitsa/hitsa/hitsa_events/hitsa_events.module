function hitsa_events_registration_submission($post)
{
    if (!empty($post['nid'])) {
        $data = array();
        $nid = $post['nid'];
        $node = node_load($nid);
        global $user;
        $uid = $user->uid;
        $serial = _webform_submission_serial_next_value($nid);
        unset($post['nid']);
        if (!empty($post['form_build_id'])) {
            unset($post['form_build_id']);
        }
        if (!empty($post['form_id'])) {
            unset($post['form_id']);
        }
        if (!empty($post['form_token'])) {
            unset($post['form_token']);
        }
        //dpm($post);
        if (!empty($post['captcha_sid'])) {
            unset($post['captcha_sid']);
        }
        if (!empty($post['captcha_token'])) {
            unset($post['captcha_token']);
        }
        if (!empty($post['captcha_response'])) {
            unset($post['captcha_response']);
        }

        foreach ($post as $post_key => $fields) {
            if (strpos($post_key, '_req') == true) {
                continue;
            }
            $key = $post_key;
            if (strpos($post_key, 'email_') !== false) {
                $key = str_replace('email_', '', $post_key);
            }
            if (strpos($post_key, 'number_') !== false) {
                $key = str_replace('number_', '', $post_key);
            }
            if (strpos($post_key, 'form_element_id') !== false) {
                $key = str_replace('form_element_id_', '', $post_key);
            }
            if (is_array($fields)) {
                foreach ($fields as $checks_values) {
                    $data[$key][] = $checks_values;
                }
            } else {
                $data[$key][] = $fields;
            }
        }
        $submission = (object) array(
            'nid' => $nid,
            'uid' => 0,
            'submitted' => REQUEST_TIME,
            'remote_addr' => ip_address(),
            'is_draft' => false,
            'serial' => $serial,
            'data' => $data,
        );
        if (module_exists('webform')) {
            module_load_include('inc', 'webform', 'includes/webform.submissions');
            $sid = webform_submission_insert($node, $submission);
        }
        if (!empty($sid)) {
            webform_submission_send_mail($node,$submission);
            return true;
        } else {
            return false;
        }
    }
}
function hitsa_events_generate_tree($periods)
{
    $tree = array();
    foreach ($periods as $period) {
        if ($period->depth == 0) {
            $tree[$period->tid] = $period;
        } elseif ($period->depth == 1) {
            $tree[$period->parents[0]]->children[$period->tid] = $period;
        }
    }
    return $tree;
}
function hitsa_events_splitter($events = null, $periods = null, $type)
{
    $periods_to_show = hitsa_events_shown_periods($periods);
    $periods_to_show = hitsa_events_set_active_period($periods_to_show);

    usort($periods_to_show, function ($a, $b) {
        return $a->field_time_period_dates['und'][0]['value'] - $b->field_time_period_dates['und'][0]['value'];
    });
    $attached_events = hitsa_events_attach_period_to_date($events, $periods_to_show, $type);
    $attached_events = hitsa_events_put_todays_date($attached_events);
    return $attached_events;
}
function hitsa_events_shown_periods($periods = null)
{
    $out = array();
    if (!empty($periods)) {
        foreach ($periods as $tid => $term) {
            if (!empty($term->children)) {
                $out[$tid] = hitsa_events_shown_periods($term->children);
            } else {
                $out[$tid] = $term;
            }
        }
    }
    return $out;
}
function hitsa_events_set_active_period($periods = null)
{
    $time = strtotime('today midnight');
    if (is_array($periods)) {
        $outer = $periods;
        foreach ($outer as $out_key => $terms) {
            if (!isset($terms->name)) {
                foreach ($terms as $tid => $term) {
                    $periods[$tid] = $term;
                    unset($periods[$out_key][$tid]);
                    if (empty($periods[$out_key])) {
                        unset($periods[$out_key]);
                    }
                }
            }

        }
    }
    if (!empty($periods)) {
        $have_active = false;
        foreach ($periods as $tid => $term) {
            if (!empty($term->field_time_period_dates)) {
                $period_dates = hitsa_events_get_period_dates($term);
            }
            if (!empty($period_dates['start']) && !empty($period_dates['end'])) {
                if ($time >= $period_dates['start'] && $period_dates['end'] >= $time) {
                  $periods[$tid]->previous = 'FALSE';
                    if ($have_active == false) {
                        $periods[$tid]->active = 'TRUE';
                        $have_active = true;
                    } else {

                        $periods[$tid]->active = 'FALSE';
                    }
                } else {
                  $periods[$tid]->previous = 'TRUE';
                    $periods[$tid]->active = 'FALSE';
                }
            }
        }
    }
    return $periods;
}
function hitsa_events_get_period_dates($term = null)
{
    $out = array();
    if (!empty($term)) {
        if (!empty($term->field_time_period_dates)) {
            $out['start'] = $term->field_time_period_dates['und'][0]['value'];
            $out['end'] = $term->field_time_period_dates['und'][0]['value2'];
        }
    }
    return $out;
}
function hitsa_events_attach_period_to_date($events = null, $periods = null, $type = null)
{
    if (!empty($events)) {
        $i = 0;
        foreach ($events as $nid => $event) {
            if (!empty($event->field_event_type)) {
                $shown = false;
                foreach ($event->field_event_type['und'] as $types) {
                    if ($types['value'] == $type) {
                        $shown = true;
                        break;
                    }
                }
                if (!$shown) {
                    continue;
                }
            }
            $ed = hitsa_events_get_event_date($event);
            $edm = hitsa_events_get_midnight($ed);
            foreach ($periods as $tid => $period) {
                $pd = hitsa_events_get_period_dates($period);
                // krumo('Sundmuse algus: ' . $ed['start']);
                // krumo('Perioodi algus: ' . $pd['start']);
                // krumo('Perioodi lopp: ' . $pd['end']);
                $in_period = 'FALSE';
                if ($edm['start'] >= $pd['start'] && $edm['start'] <= $pd['end']) {
                    $in_period = 'TRUE';
                }
                // kui tahame syndmust tulevikku ka kuvatud
                // elseif($ed['end']<=$pd['end'] && $pd['start']<=$ed{'end'}){
                //     $in_period = 'TRUE';
                // }
                // elseif($ed['start']<=$pd['start']&&$ed['end']>=$pd['end']){
                //     $in_period= 'TRUE';
                // }
                if ($in_period == 'TRUE') {

                    $events_html = hitsa_events_html_decider($event, $ed, $pd);
                    foreach ($events_html as $event_html) {
                        $periods[$tid]->events[] = $event_html;
                    }
                }
            }
            if ($i == 40) {
                // break;
            }
            $i++;
        }
    }
    return $periods;
}

function hitsa_events_get_event_date($event = null)
{
    $out = array();
    if (!empty($event)) {
        if (!empty($event->event_date)) {
            $out['start'] = $event->event_date['und'][0]['value'];
            $out['end'] = $event->event_date['und'][0]['value2'];
        }
    }
    return $out;
}
function hitsa_events_put_todays_date($events = null)
{
    $time = strtotime('yesterday 23:59:59');
    $time_real = strtotime('today midnight');
    foreach ($events as $per_key => $period) {
        if ($period->active == 'TRUE') {
            $part = '<div class="object-dateseparator"><span>' . t('today') . ', ' . format_date($time_real, 'custom', 'd.F') . '</span></div>';
            $events[$per_key]->events['today']['dates'] = $time;
            $events[$per_key]->events['today']['sort_date'] = $time;
            $events[$per_key]->events['today']['today'] = true;
            $events[$per_key]->events['today']['html'] = $part;
        }
    }
    return $events;
}

function hitsa_events_event_day_splitter($start = null, $end = null)
{
    if (!empty($start)) {
        $numDays = abs($end - $start) / 60 / 60 / 24;
    }
    ;
    return $numDays;
}
function hitsa_events_html_decider($event = null, $dates = null, $pd = null)
{
    $event_html_out = array();
    if (!empty($dates) && !empty($event)) {
        if (isset($dates['start']) && isset($dates['end'])) {
            if ($dates['start'] == $dates['end']) {
                $event_html = hitsa_events_event_html($event);
                $event_html['sort_date'] = $dates['start'];
                $event_html_out[] = $event_html;
            } else {

                if ($dates['end'] >= $pd['end']) {
                    $dates['end'] = $pd['end'];
                    $dates['end'];
                }
                if ($dates['start'] <= $pd['start']) {
                    $dates['start'] = $pd['start'];
                }
                $numDays = hitsa_events_event_day_splitter($dates['start'], $dates['end']);
                for ($i = 0; $i < 1; $i++) {

                    $start_date_date = date('U', strtotime("+{$i} day", $dates['start']));
                    if (!empty($_REQUEST)) {
                        if (!empty($_REQUEST['date'])) {
                            $two_dates = explode(' - ', $_REQUEST['date']);

                            $two_dates[0] = strtotime(str_replace('/', '-', $two_dates[0]));
                            $two_dates[1] = strtotime(str_replace('/', '-', $two_dates[1]));
                            if ($two_dates[0] > $start_date_date) {
                                continue;
                            }
                            if ($two_dates[1] < $start_date_date) {
                                continue;
                            }
                        }
                    }
                    $event_html = hitsa_events_event_html($event, $type = 'event', $start_date_date);
                    $event_html['dates'] = $dates;
                    $event_html['sort_date'] = $start_date_date;
                    $event_html_out[] = $event_html;

                }

            }
        }
    }
    return $event_html_out;
}
function hitsa_events_sort_period_events($periods)
{

    foreach ($periods as $tid => $period) {
        if (!empty($period->events)) {
            usort($period->events, function ($a, $b) {
                return $a['sort_date'] - $b['sort_date'];
            });
        } else {
            unset($periods[$tid]);
        }
        if (isset($periods[$tid])) {
            $periods[$tid] = $period;
        }
    }
    return $periods;
}
function hitsa_events_get_midnight($dates)
{
    if (!empty($dates)) {
        foreach ($dates as $key => $date) {
            $dates[$key] = strtotime('midnight ', $date);
        }
    }
    return $dates;
}