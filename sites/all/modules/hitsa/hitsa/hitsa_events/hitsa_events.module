function hitsa_events_for_services()
{
    global $language;
    $time = strtotime('today midnight');
    $events = hitsa_core_get_content_by_type('node', 'event', $language->language, $conditions = array('event_type' => 'training'));
    if (isset($events['node'])) {
        $event_nids = array_keys($events['node']);
        $events = entity_load('node', $event_nids);
    }
    $new_events = array();
    if (!empty($events)) {
        foreach ($events as $event) {
            if (!empty($event->event_date)) {
                $start = $event->event_date['und'][0]['value'];
                $end = $event->event_date['und'][0]['value2'];
                if ($start >= $time) {
                    $new_events[] = $event;
                } elseif ($time <= $end) {
                    $new_events[] = $event;
                }
            }
        }
    }
    if (!empty($new_events)) {
        $output = '';
        $events_count = count($new_events);
        $i = 0;
        usort($new_events, function ($a, $b) {
            return $a->event_date['und'][0]['value'] - $b->event_date['und'][0]['value'];
        });
        if (!empty($new_events)) {
            $output .= ' <div data-tab="tab-2">';
            $output .= '             <div class="row">';
        }
        foreach ($new_events as $event) {
            $start = $event->event_date['und'][0]['value'];
            $end = $event->event_date['und'][0]['value2'];
            $start_out = date('d.m', $start);
            $end_out = date('d.m', $end);
            $date_out = '';
            $price = null;
            if ($start == $end) {
                $date_out = $start_out;
            } else {
                $date_out = $start_out . ' - ' . $end_out;
            }

            if (!empty($event->field_training_cost)) {
                $price = $event->field_training_cost['und'][0]['value'];
            }
            $output .= '<div class="col-12">';
            $output .= '   <a href="' . url('training-calendar') . '#anchor=' . rawurlencode($event->nid) . '" class="object object-small object-horizontal">';
            $output .= '      <span class="object-inner">';
            $output .= '         <span class="object-content">';
            $output .= '            <span class="object-title">' . $event->title . '</span>';
            $output .= '            <span class="object-footer">';
            $output .= '               <span class="before-calendar">' . $date_out . '</span>';
            $output .= (!empty($price)) ? '               <span class="before-money">' . $price . ' â‚¬</span>' : '';
            $output .= '            </span><!--/object-footer-->';
            $output .= '         </span><!--/object-content-->';
            $output .= '      </span><!--/object-inner-->';
            $output .= '   </a><!--/object-->';
            $output .= '</div><!--/col-12-->';
            if ($i >= 0) {
                $output .= '<hr>';
            }
            if ($i == 4) {
                break;
            }
            $i++;
        }
        if (!empty($new_events)) {
            $output .= '<div class="col-12 col-margin">';
            $output .= '                  <a href="' . url('training-calendar') . '" class="btn btn-filled btn-stretch">' . t('Training calendar') . '</a>';
            $output .= '               </div><!--/col-12-->';
            $output .= '</div><!--/row-->';
            $output .= '         </div><!--/data-tab-->';
        }
        return $output;
    }
}

function hitsa_events_htmlize_periods($periods)
{
    $output = '';

    $accordion_start = '<div class="accordion" data-allClosed="TRUE" data-plugin="accordion">';
    $previous_periods = array();
    foreach ($periods as $period) {
      if($period->previous=='TRUE'){
        $previous_periods[] = $period;
      }
      else {
        if (strval($period->active) == 'TRUE') {
          $output .= '<div class="accordion-entry accordion-active">';
        } else {
          $output .= '<div class="accordion-entry">';
        }

        $events_count = count($period->events);
        $events_count_start = $events_count;
        if (strval($period->active) == 'TRUE') {
          $events_count--;
        }
        
        $output_content = '<div class="accordion-content">';

        $i = 0;

        $time = strtotime('today midnight');
        $lastElement = end($period->events);

        foreach ($period->events as $key => $event_html) {
          if (strval($period->active) == 'TRUE') {
            if ($event_html['start_date'] < $time && !isset($event_html['today'])) {
              if(isset($previous_periods[$period->tid])){
                $previous_periods[$period->tid]->events[] = $event_html;
              }
              else{
                $previous_periods[$period->tid] = $period;
                unset($previous_periods[$period->tid]->events);
                $previous_periods[$period->tid]->events[] = $event_html;
              }
//              dpm($period->events[$key]);
              continue;
            }
          }
          if (isset($event_html['today'])) {
            if (isset($period->events[$key - 1])) {
            }
          }
          $output_content .= $event_html['html'];
          if (!isset($event_html['today'])) {
            if($event_html != $lastElement) {
              $output_content .= '<hr />';
            }
          }
          if (isset($event_html['today'])) {
            if (isset($period->events[$key - 1])) {
            }
          } else {
            if (isset($period->events[$key + 1])) {
              if (!isset($period->events[$key + 1]['today'])) {
                $output_content .= '<hr />';
              }
            }
          }
          $i++;
        }
        $output_content .= '</div><!--/accordion-content-->';
        $events_count = count($period->events);
        $events_count = $events_count_start-$events_count;
        $output_title = '<div class="accordion-title">' . $period->name . '</div>';
        $output .= $output_title.$output_content;
        $output .= '</div><!--/accordion-entry-->';
      }
    }
//    if(!empty($previous_periods)){
//      $output = '<div class="accordion-entry accordion-entry-past-events">
//                           <div class="accordion-title">MÃ¶Ã¶dunud sÃ¼ndmused</div>
//                           </div>
//
//                           <div class="accordion-content"></div>
//                           '.$output;
//    }

    $accordion_end = '</div><!--/accordion-->';
    $previous_semesters = '';
    if(!empty($previous_periods)){

      $previous_semester = '';
      usort($previous_periods, function ($a, $b) {
        return $a->field_time_period_dates['und'][0]['value2'] - $b->field_time_period_dates['und'][0]['value2'];
      });
      foreach ($previous_periods as $prev){
        $events_count = count($prev->events);
        $previous_semester .= '<div class="accordion-entry">';
        $previous_semester .= '<div class="accordion-title">' . $prev->name . '(' . $events_count . ')</div>';
        $previous_semester .= '<div class="accordion-content">';
        foreach ($prev->events as $key => $event_html) {
          $previous_semester .= $event_html['html'];
          if (isset($event_html['today'])) {
            if (isset($prev->events[$key - 1])) {
            }
          } else {
            if (isset($prev->events[$key + 1])) {
              if (!isset($prev->events[$key + 1]['today'])) {
                $previous_semester .= '<hr />';
              }
            }
          }
        }

        $previous_semester .= '</div>';
        $previous_semester .='</div>';
      }
      $previous_semesters = '<div class="accordion-entry accordion-entry-past-events">
                           <div class="accordion-title">'.t('Previous events').'</div>
                           
                           <div class="accordion-content">'.$previous_semester.'</div>
                           </div>

                           ';
    }
    $output = $accordion_start.$previous_semesters.$output.$accordion_end;
//    $output.= theme('semester_accordion');
    return $output;
}

function hitsa_events_autocomplete()
{
    global $language;
    $q = db_select('node', 'n');
    $q->condition('n.type', 'event', '=');
    $q->condition('n.language', $language->language, '=');
    if (!empty($_REQUEST)) {
        if (!empty($_REQUEST['search'])) {
            $q->condition('n.title', '%' . $_REQUEST['search'] . '%', 'LIKE');
        }
        if (!empty($_REQUEST['type'])) {
            $q->join('field_data_field_event_type', 'fdfet', 'n.nid=fdfet.entity_id');
            $q->condition('fdfet.field_event_type_value', $_REQUEST['type'], '=');
        }
    }
    $q->fields('n');

    $result = $q->execute();
    $data = array();
    $data['list'] = array();
    foreach ($result as $node) {
