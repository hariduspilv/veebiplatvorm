            }
            if ($key == 'Huvitegevus') {
                $cal_link = '<a  href="' . $link . '" class="btn btn-filled sm-stretch">' . t('Hobby calendar') . '</a>';
            } elseif ($key == 'Ã•ppetÃ¶Ã¶') {
